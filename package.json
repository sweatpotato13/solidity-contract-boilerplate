{
  "name": "solidity-contract-boilerplate",
  "version": "2.4.2",
  "main": "index.js",
  "author": "CuteWisp",
  "license": "BUSL-1.1",
  "files": [
    "artifacts",
    "typechain-types"
  ],
  "scripts": {
    "build": "npx hardhat compile",
    "coverage": "npx hardhat coverage",
    "deploy": "npx hardhat deploy --network",
    "lint:typescript": "eslint 'test/**/*.ts' 'scripts/**/*.ts'",
    "lint:typescript:fix": "prettier --config .prettierrc.json --write 'scripts/**/*.ts' 'test/**/*.ts' 'hardhat.config.ts'",
    "lint:solidity": "npx solhint 'src/contracts/**/*.sol'",
    "lint:solidity:fix": "prettier --write --plugin=prettier-plugin-solidity 'src/contracts/**/*.sol'",
    "lint": "yarn run lint:typescript && yarn run lint:solidity",
    "lint:fix": "yarn run lint:typescript:fix && yarn run lint:solidity:fix",
    "test": "REPORT_GAS=true npx hardhat test"
  },
  "devDependencies": {
    "@babel/core": "^7.24.5",
    "@chainsafe/eslint-config": "^2.2.4",
    "@metamask/eth-sig-util": "^7.0.2",
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.6",
    "@nomicfoundation/hardhat-ethers": "^3.0.5",
    "@nomicfoundation/hardhat-ignition": "^0.15.2",
    "@nomicfoundation/hardhat-ignition-ethers": "^0.15.2",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.10",
    "@nomicfoundation/hardhat-toolbox": "^5.0.0",
    "@nomicfoundation/hardhat-verify": "^2.0.6",
    "@nomicfoundation/ignition-core": "^0.15.2",
    "@openzeppelin/contracts": "5.0.2",
    "@rushstack/eslint-patch": "^1.10.2",
    "@typechain/ethers-v6": "^0.5.1",
    "@typechain/hardhat": "^9.1.0",
    "@types/babel__core": "^7",
    "@types/chai": "^4.3.15",
    "@types/mocha": ">=10.0.6",
    "@types/node": ">=20.12.8",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "chai": "^4.2.0",
    "chain": "^0.4.0",
    "dotenv": "^16.4.5",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "ethers": "^6.12.1",
    "hardhat-deploy": "^0.12.4",
    "hardhat-gas-reporter": "^1.0.8",
    "hardhat-preprocessor": "^0.1.5",
    "prettier": "^3.2.5",
    "prettier-plugin-solidity": "^1.3.1",
    "sinon": "^17.0.1",
    "solhint": "^4.5.4",
    "solidity-coverage": "^0.8.12",
    "ts-node": ">=10.9.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "5.4.5"
  },
  "dependencies": {
    "hardhat": "^2.22.3",
    "typechain": "^8.3.2"
  },
  "packageManager": "yarn@4.2.1"
}
